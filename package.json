{
	"name": "editanywhere-vscode",
	"displayName": "EditAnywhere",
	"description": "EditAnywhere client for vscode",
	"version": "0.1.0",
	"publisher": "Super Evil Mega Co",
	"repository": "https://github.com/superevilmegaco/EditAnywhere-vscode",
	"license": "MIT",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:EditAnywhere.ListApps"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "EditAnywhere.ListApps",
				"title": "EditAnywhere"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0",
		"typescript": "^3.9.4",
		"@types/vscode": "^1.32.0"
	},
	"dependencies": {
		"@types/mime-types": "^2.1.0",
		"@types/node-fetch": "^2.5.7",
		"mime-types": "^2.1.27",
		"node-fetch": "^2.6.0"
	}
}
